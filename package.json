{
  "name": "fsd-flat-starter-kit",
  "version": "1.0.0",
  "description": "Starter Kit for simple HTML+CSS pages (flat-pages) built by FSD team",
  "engines": {
    "node": ">=7.0.0"
  },
  "scripts": {
    "dev": "better-npm-run start-dev",
    "prod": "better-npm-run build-prod",
    "test": "better-npm-run start-test"
  },
  "betterScripts": {
    "build-prod": {
      "command": "./node_modules/.bin/webpack",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "./node_modules/.bin/webpack-dev-server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "start-test": {
      "command": "./node_modules/.bin/webpack-dev-server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "makeup",
        "PORT": 8090
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullstack-development/flat-starter-kit.git"
  },
  "keywords": [
    "Starter Kit",
    "Flat Pages",
    "Pug",
    "Stylus",
    "Webpack",
    "BEM"
  ],
  "author": "Fullstack Development Llc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fullstack-development/flat-starter-kit/issues"
  },
  "homepage": "https://github.com/fullstack-development/flat-starter-kit#readme",
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "better-npm-run": "0.0.13",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.25.0",
    "doiuse": "^2.6.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.9.0",
    "html-webpack-harddisk-plugin": "0.0.2",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1",
    "node-sass": "^4.5.2",
    "normalize.css": "^5.0.0",
    "peity": "^3.2.1",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
    "postcss-scss": "^0.4.1",
    "pug": "^2.0.0-beta11",
    "pug-html-loader": "^1.1.4",
    "pug-loader": "^2.3.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2",
    "stylelint": "^7.9.0",
    "stylelint-order": "^0.4.2",
    "webpack": "^2.3.2",
    "webpack-config": "^6.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.17.1"
  },
  "devDependencies": {
    "makeup": "^1.0.1",
    "script-loader": "^0.7.0"
  }
}
